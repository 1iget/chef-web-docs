<script>
  var mainTemplate = document.getElementById('app-content');
  window.mainTemplate = mainTemplate.textContent;
  window.baseUrl = <%= learn_chef_url.to_json %>;
  window.currentPage = {
    url: <%= current_page.url.to_json %>,
    id: <%= current_page.id.to_json %>,
    section: <%= get_page_section(current_page)[0].to_json %>,
    quiz: <%= get_quiz_data(current_page).to_json %>
  };
</script>
