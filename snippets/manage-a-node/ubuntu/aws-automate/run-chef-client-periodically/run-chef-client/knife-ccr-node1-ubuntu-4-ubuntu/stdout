172.31.54.179     +running_pid() {
172.31.54.179     +  pid=$1
172.31.54.179     +  name=$2
172.31.54.179     +  [ -z "$pid" ] && return 1
172.31.54.179     +  [ ! -d /proc/$pid ] &&  return 1
172.31.54.179     +  cmd=`cat /proc/$pid/cmdline | tr '\000' '\n' | awk 'NR==2'`
172.31.54.179     +  [ "$cmd" != "$name" ] &&  return 1
172.31.54.179     +  return 0
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +running() {
172.31.54.179     +  [ ! -f "$PIDFILE" ] && return 1
172.31.54.179     +  pid=`cat $PIDFILE`
172.31.54.179     +  running_pid $pid $DAEMON || return 1
172.31.54.179     +  return 0
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +start_server() {
172.31.54.179     +  if [ -z "$DAEMONUSER" ] ; then
172.31.54.179     +    start_daemon -p $PIDFILE $DAEMON $DAEMON_OPTS
172.31.54.179     +  errcode=$?
172.31.54.179     +  else
172.31.54.179     +    start-stop-daemon --start --quiet --pidfile $PIDFILE \
172.31.54.179     +      --chuid $DAEMONUSER \
172.31.54.179     +      --exec $DAEMON -- $DAEMON_OPTS
172.31.54.179     +    errcode=$?
172.31.54.179     +  fi
172.31.54.179     +  return $errcode
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +stop_server() {
172.31.54.179     +   if [ -z "$DAEMONUSER" ] ; then
172.31.54.179     +     killproc -p $PIDFILE $DAEMON
172.31.54.179     +     errcode=$?
172.31.54.179     +   else
172.31.54.179     +     start-stop-daemon --stop --quiet --pidfile $PIDFILE \
172.31.54.179     +       --user $DAEMONUSER \
172.31.54.179     +       --exec $DAEMON
172.31.54.179     +     errcode=$?
172.31.54.179     +   fi
172.31.54.179     +   return $errcode
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +reload_server() {
172.31.54.179     +  if [ -z "$DAEMONUSER" ] ; then
172.31.54.179     +     killproc -p $PIDFILE $DAEMON -HUP
172.31.54.179     +     errcode=$?
172.31.54.179     +   else
172.31.54.179     +     start-stop-daemon --stop --signal HUP --quiet --pidfile $PIDFILE \
172.31.54.179     +       --user $DAEMONUSER \
172.31.54.179     +       --exec $DAEMON
172.31.54.179     +     errcode=$?
172.31.54.179     +   fi
172.31.54.179     +   return $errcode
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +run_server() {
172.31.54.179     +  if [ -z "$DAEMONUSER" ] ; then
172.31.54.179     +     killproc -p $PIDFILE $DAEMON -USR1
172.31.54.179     +     errcode=$?
172.31.54.179     +   else
172.31.54.179     +     start-stop-daemon --stop --signal USR1 --quiet --pidfile $PIDFILE \
172.31.54.179     +       --user $DAEMONUSER \
172.31.54.179     +       --exec $DAEMON
172.31.54.179     +     errcode=$?
172.31.54.179     +   fi
172.31.54.179     +   return $errcode
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +force_stop() {
172.31.54.179     +  [ ! -e "$PIDFILE" ] && return
172.31.54.179     +  if running ; then
172.31.54.179     +    /bin/kill -15 $pid
172.31.54.179     +    sleep "$DIETIME"s
172.31.54.179     +    if running ; then
172.31.54.179     +      /bin/kill -9 $pid
172.31.54.179     +      sleep "$DIETIME"s
172.31.54.179     +      if running ; then
172.31.54.179     +        echo "Cannot kill $NAME (pid=$pid)!"
172.31.54.179     +        exit 1
172.31.54.179     +      fi
172.31.54.179     +    fi
172.31.54.179     +  fi
172.31.54.179     +  rm -f $PIDFILE
172.31.54.179     +}
172.31.54.179     +
172.31.54.179     +case "$1" in
172.31.54.179     +  start)
172.31.54.179     +    log_daemon_msg "Starting $DESC " "$NAME"
172.31.54.179     +    if running ;  then
172.31.54.179     +        log_progress_msg "apparently already running"
172.31.54.179     +        log_end_msg 0
172.31.54.179     +        exit 0
172.31.54.179     +    fi
172.31.54.179     +    if start_server ; then
172.31.54.179     +        [ -n "$STARTTIME" ] && sleep $STARTTIME # Wait some time
172.31.54.179     +        if  running ;  then
172.31.54.179     +            log_end_msg 0
172.31.54.179     +        else
172.31.54.179     +            log_end_msg 1
172.31.54.179     +        fi
172.31.54.179     +    else
172.31.54.179     +        log_end_msg 1
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  stop)
172.31.54.179     +    log_daemon_msg "Stopping $DESC" "$NAME"
172.31.54.179     +    if running ; then
172.31.54.179     +      errcode=0
172.31.54.179     +      stop_server || errcode=$?
172.31.54.179     +      log_end_msg $errcode
172.31.54.179     +    else
172.31.54.179     +      log_progress_msg "apparently not running"
172.31.54.179     +      log_end_msg 0
172.31.54.179     +      exit 0
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  force-stop)
172.31.54.179     +    $0 stop
172.31.54.179     +    if running; then
172.31.54.179     +      log_daemon_msg "Stopping (force) $DESC" "$NAME"
172.31.54.179     +      errcode=0
172.31.54.179     +      force_stop || errcode=$?
172.31.54.179     +      log_end_msg $errcode
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  restart|force-reload)
172.31.54.179     +    log_daemon_msg "Restarting $DESC" "$NAME"
172.31.54.179     +    errcode=0
172.31.54.179     +    stop_server || errcode=$?
172.31.54.179     +    [ -n "$DIETIME" ] && sleep $DIETIME
172.31.54.179     +    start_server || errcode=$?
172.31.54.179     +    [ -n "$STARTTIME" ] && sleep $STARTTIME
172.31.54.179     +    running || errcode=$?
172.31.54.179     +    log_end_msg $errcode
172.31.54.179     +    ;;
172.31.54.179     +  status)
172.31.54.179     +    log_daemon_msg "Checking status of $DESC" "$NAME"
172.31.54.179     +    if running ;  then
172.31.54.179     +      log_progress_msg "running"
172.31.54.179     +      log_end_msg 0
172.31.54.179     +    else
172.31.54.179     +      log_progress_msg "apparently not running"
172.31.54.179     +      log_end_msg 1
172.31.54.179     +      exit 3
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  reload)
172.31.54.179     +    if running; then
172.31.54.179     +      log_daemon_msg "Reloading $DESC" "$NAME"
172.31.54.179     +      errcode=0
172.31.54.179     +      reload_server || errcode=$?
172.31.54.179     +      log_end_msg $errcode
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  run)
172.31.54.179     +    if running; then
172.31.54.179     +      log_daemon_msg "Triggering run of $DESC" "$NAME"
172.31.54.179     +      errcode=0
172.31.54.179     +      run_server || errcode=$?
172.31.54.179     +      log_end_msg $errcode
172.31.54.179     +    fi
172.31.54.179     +    ;;
172.31.54.179     +  *)
172.31.54.179     +    N=/etc/init.d/$NAME
172.31.54.179     +    echo "Usage: $N {start|stop|force-stop|restart|force-reload|status|run}" >&2
172.31.54.179     +    exit 1
172.31.54.179     +    ;;
172.31.54.179     +esac
172.31.54.179     +
172.31.54.179     +exit 0
