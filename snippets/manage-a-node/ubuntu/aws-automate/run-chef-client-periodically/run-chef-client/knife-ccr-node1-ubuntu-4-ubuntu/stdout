172.31.56.2     +running_pid() {
172.31.56.2     +  pid=$1
172.31.56.2     +  name=$2
172.31.56.2     +  [ -z "$pid" ] && return 1
172.31.56.2     +  [ ! -d /proc/$pid ] &&  return 1
172.31.56.2     +  cat /proc/$pid/cmdline | tr '\000' ' ' | grep -q "$name"
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +running() {
172.31.56.2     +  [ ! -f "$PIDFILE" ] && return 1
172.31.56.2     +  pid=`cat $PIDFILE`
172.31.56.2     +  running_pid $pid $DAEMON || return 1
172.31.56.2     +  return 0
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +start_server() {
172.31.56.2     +  if [ -z "$DAEMONUSER" ] ; then
172.31.56.2     +    start_daemon -p $PIDFILE $DAEMON $DAEMON_OPTS
172.31.56.2     +  errcode=$?
172.31.56.2     +  else
172.31.56.2     +    start-stop-daemon --start --quiet --pidfile $PIDFILE \
172.31.56.2     +      --chuid $DAEMONUSER \
172.31.56.2     +      --exec $DAEMON -- $DAEMON_OPTS
172.31.56.2     +    errcode=$?
172.31.56.2     +  fi
172.31.56.2     +  return $errcode
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +stop_server() {
172.31.56.2     +   if [ -z "$DAEMONUSER" ] ; then
172.31.56.2     +     killproc -p $PIDFILE $DAEMON
172.31.56.2     +     errcode=$?
172.31.56.2     +   else
172.31.56.2     +     start-stop-daemon --stop --quiet --pidfile $PIDFILE \
172.31.56.2     +       --user $DAEMONUSER \
172.31.56.2     +       --exec $DAEMON
172.31.56.2     +     errcode=$?
172.31.56.2     +   fi
172.31.56.2     +   return $errcode
172.31.56.2     +}
