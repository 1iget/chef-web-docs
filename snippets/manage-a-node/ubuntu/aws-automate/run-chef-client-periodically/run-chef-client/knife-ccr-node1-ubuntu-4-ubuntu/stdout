172.31.56.2     +reload_server() {
172.31.56.2     +  if [ -z "$DAEMONUSER" ] ; then
172.31.56.2     +     killproc -p $PIDFILE $DAEMON -HUP
172.31.56.2     +     errcode=$?
172.31.56.2     +   else
172.31.56.2     +     start-stop-daemon --stop --signal HUP --quiet --pidfile $PIDFILE \
172.31.56.2     +       --user $DAEMONUSER \
172.31.56.2     +       --exec $DAEMON
172.31.56.2     +     errcode=$?
172.31.56.2     +   fi
172.31.56.2     +   return $errcode
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +run_server() {
172.31.56.2     +  if [ -z "$DAEMONUSER" ] ; then
172.31.56.2     +     killproc -p $PIDFILE $DAEMON -USR1
172.31.56.2     +     errcode=$?
172.31.56.2     +   else
172.31.56.2     +     start-stop-daemon --stop --signal USR1 --quiet --pidfile $PIDFILE \
172.31.56.2     +       --user $DAEMONUSER \
172.31.56.2     +       --exec $DAEMON
172.31.56.2     +     errcode=$?
172.31.56.2     +   fi
172.31.56.2     +   return $errcode
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +force_stop() {
172.31.56.2     +  [ ! -e "$PIDFILE" ] && return
172.31.56.2     +  if running ; then
172.31.56.2     +    /bin/kill -15 $pid
172.31.56.2     +    sleep "$DIETIME"s
172.31.56.2     +    if running ; then
172.31.56.2     +      /bin/kill -9 $pid
172.31.56.2     +      sleep "$DIETIME"s
172.31.56.2     +      if running ; then
172.31.56.2     +        echo "Cannot kill $NAME (pid=$pid)!"
172.31.56.2     +        exit 1
172.31.56.2     +      fi
172.31.56.2     +    fi
172.31.56.2     +  fi
172.31.56.2     +  rm -f $PIDFILE
172.31.56.2     +}
172.31.56.2     +
172.31.56.2     +case "$1" in
172.31.56.2     +  start)
172.31.56.2     +    log_daemon_msg "Starting $DESC " "$NAME"
172.31.56.2     +    if running ;  then
172.31.56.2     +        log_progress_msg "apparently already running"
172.31.56.2     +        log_end_msg 0
172.31.56.2     +        exit 0
172.31.56.2     +    fi
172.31.56.2     +    if start_server ; then
172.31.56.2     +        [ -n "$STARTTIME" ] && sleep $STARTTIME # Wait some time
172.31.56.2     +        if  running ;  then
172.31.56.2     +            log_end_msg 0
172.31.56.2     +        else
172.31.56.2     +            log_end_msg 1
172.31.56.2     +        fi
172.31.56.2     +    else
172.31.56.2     +        log_end_msg 1
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  stop)
172.31.56.2     +    log_daemon_msg "Stopping $DESC" "$NAME"
172.31.56.2     +    if running ; then
172.31.56.2     +      errcode=0
172.31.56.2     +      stop_server || errcode=$?
172.31.56.2     +      log_end_msg $errcode
172.31.56.2     +    else
172.31.56.2     +      log_progress_msg "apparently not running"
172.31.56.2     +      log_end_msg 0
172.31.56.2     +      exit 0
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  force-stop)
172.31.56.2     +    $0 stop
172.31.56.2     +    if running; then
172.31.56.2     +      log_daemon_msg "Stopping (force) $DESC" "$NAME"
172.31.56.2     +      errcode=0
172.31.56.2     +      force_stop || errcode=$?
172.31.56.2     +      log_end_msg $errcode
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  restart|force-reload)
172.31.56.2     +    log_daemon_msg "Restarting $DESC" "$NAME"
172.31.56.2     +    errcode=0
172.31.56.2     +    stop_server || errcode=$?
172.31.56.2     +    [ -n "$DIETIME" ] && sleep $DIETIME
172.31.56.2     +    start_server || errcode=$?
172.31.56.2     +    [ -n "$STARTTIME" ] && sleep $STARTTIME
172.31.56.2     +    running || errcode=$?
172.31.56.2     +    log_end_msg $errcode
172.31.56.2     +    ;;
172.31.56.2     +  status)
172.31.56.2     +    log_daemon_msg "Checking status of $DESC" "$NAME"
172.31.56.2     +    if running ;  then
172.31.56.2     +      log_progress_msg "running"
172.31.56.2     +      log_end_msg 0
172.31.56.2     +    else
172.31.56.2     +      log_progress_msg "apparently not running"
172.31.56.2     +      log_end_msg 1
172.31.56.2     +      exit 3
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  reload)
172.31.56.2     +    if running; then
172.31.56.2     +      log_daemon_msg "Reloading $DESC" "$NAME"
172.31.56.2     +      errcode=0
172.31.56.2     +      reload_server || errcode=$?
172.31.56.2     +      log_end_msg $errcode
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  run)
172.31.56.2     +    if running; then
172.31.56.2     +      log_daemon_msg "Triggering run of $DESC" "$NAME"
172.31.56.2     +      errcode=0
172.31.56.2     +      run_server || errcode=$?
172.31.56.2     +      log_end_msg $errcode
172.31.56.2     +    fi
172.31.56.2     +    ;;
172.31.56.2     +  *)
172.31.56.2     +    N=/etc/init.d/$NAME
172.31.56.2     +    echo "Usage: $N {start|stop|force-stop|restart|force-reload|status|run}" >&2
172.31.56.2     +    exit 1
172.31.56.2     +    ;;
172.31.56.2     +esac
172.31.56.2     +
172.31.56.2     +exit 0
